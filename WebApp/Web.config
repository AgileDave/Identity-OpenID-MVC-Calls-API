<configuration>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    
    <!-- These are used by the MVC app in order to obtain an identity token for logging in -->
    <add key="ida:ClientId" value="[MVC App Client ID]" />
    <add key="ida:ClientSecret" value="[MVC App Client Secret]" />
    <add key="ida:AADInstance" value="https://login.microsoftonline.com/{0}{1}" />
    <add key="ida:RedirectUri" value="https://localhost:44326/" />
    <add key="ida:AADTenant" value="[MVC App Tenant, such as contoso.onmicrosoft.com]" />
    <!-- End MVC app identity token settings -->
    
    <!-- These are used to get an access token to call the Web API service -->
    <add key="WebApiTodoListBaseAddress" value="https://localhost:44372"/>
    <add key="WebApiTodoListScope" value="api://[Web API Client ID]/.default"/>
    <!-- End Web API serivce token config settings -->
    
    <add key="vs:EnableBrowserLink" value="false" />
  </appSettings>
